    "astw": "^1.2.0",
    "escodegen": "^1.4.1",
    "is-require": "0.0.1",
    "resolve": "^1.0.0",
    "shallow-copy": "0.0.1",
    "sleuth": "^0.1.1",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "smokestack": "^2.0.0",
    "tap-spec": "^1.0.1",
    "tape": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hughsk/fresh-require.git"
  },
  "keywords": [
